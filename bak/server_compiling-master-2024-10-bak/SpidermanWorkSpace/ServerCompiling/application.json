          "maintainer": "Bob Brown",
          "createdTime": "2023-10-20T09:00:00Z",
          "resourceRequirements": {
            "cpu": "1 core",
            "memory": "1 GB",
            "disk": "5 GB"
          },
          "operatingSystem": "Ubuntu 20.04 LTS",
          "topics": [
            {
              "name": "vehicle/speed",
              "type": "float32",
              "direction": "publish"
            },
            {
              "name": "vehicle/fuelLevel",
              "type": "float32",
              "direction": "publish"
            },
            {
              "name": "vehicle/engineStatus",
              "type": "string",
              "direction": "publish"
            },
            {
              "name": "vehicle/status",
              "type": {
                "speed": "float32",
                "fuelLevel": "float32",
                "engineStatus": "string"
              },
              "direction": "publish"
            }
          ]
        },
        {
          "name": "DataAnalyzer",
          "type": "data_analysis",
