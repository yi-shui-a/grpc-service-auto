            "disk": "8 GB"
          },
          "operatingSystem": "Ubuntu 20.04 LTS",
          "topics": [
            {
              "name": "vehicle/speed",
              "type": "float32",
              "direction": "subscribe"
            },
            {
              "name": "vehicle/fuelLevel",
              "type": "float32",
              "direction": "subscribe"
            },
            {
              "name": "vehicle/engineStatus",
              "type": "string",
              "direction": "subscribe"
            },
            {
              "name": "vehicle/status",
              "type": {
                "speed": "float32",
                "fuelLevel": "float32",
                "engineStatus": "string"
              },
              "direction": "subscribe"
            },
            {
              "name": "analysis/alerts",
              "type": "string",
              "direction": "publish"
            }
          ]
        },
        {
          "name": "AlertManager",
