    "services": [
      {
        "name": "VehicleDataCollector",
        "type": "data_collection",
        "description": "Collects data from vehicles including speed, fuel level, and engine status.",
        "topics": [
          {
            "name": "vehicle/speed",
            "type": "float32",
            "direction": "publish"
          },
          {
            "name": "vehicle/fuelLevel",
            "type": "float32",
            "direction": "publish"
          },
          {
            "name": "vehicle/engineStatus",
            "type": "string",
            "direction": "publish"
          },
          {
            "name": "vehicle/status",
            "type": {
              "speed": "float32",
              "fuelLevel": "float32",
              "engineStatus": "string"
            },
            "direction": "publish"
          }
        ]
      },
      {
        "name": "DataAnalyzer",
        "type": "data_analysis",
        "description": "Analyzes collected vehicle data to provide insights and alerts.",
        "topics": [
          {
            "name": "vehicle/speed",
            "type": "float32",
            "direction": "subscribe"
          },
          {
            "name": "vehicle/fuelLevel",
            "type": "float32",
            "direction": "subscribe"
          },
          {
            "name": "vehicle/engineStatus",
            "type": "string",
            "direction": "subscribe"
          },
          {
            "name": "vehicle/status",
            "type": {
              "speed": "float32",
              "fuelLevel": "float32",
              "engineStatus": "string"
            },
            "direction": "subscribe"
          },
          {
            "name": "analysis/alerts",
            "type": "string",
            "direction": "publish"
          }
        ]
      },
      {
        "name": "AlertManager",
        "type": "alert_management",
        "description": "Manages alerts generated by the DataAnalyzer and sends notifications.",
        "topics": [
          {
            "name": "analysis/alerts",
            "type": "string",
            "direction": "subscribe"
          },
          {
            "name": "alerts/notifications",
            "type": "string",
            "direction": "publish"
          }
        ]
      }
    ],
    "communicationProtocol": "DDS",
    "communicationDescription": "Services communicate via DDS using predefined topics. Each service can publish or subscribe to one or more topics to exchange data. Structured data types are supported for complex messages."
